{
  "name": "autoapply2",
  "version": "1.0",
  "description": "auto apply2",
  "permissions": ["contextMenus", "tabs",  "notifications", "storage", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js", "config.js"],
    "persistent": false
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content.js", "config.js"]
    }
  ],

  "web_accessible_resources": ["index.html"],
  
  "manifest_version": 2
}
